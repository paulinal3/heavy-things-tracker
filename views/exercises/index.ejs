<head>
    <link rel="stylesheet" href="/static/css/exercises.css">
</head>

<body>
    <header>
        <nav>
            <%- include('../partials/userNavBar') %>
        </nav>
        <div id="res-header">
            <h1>Exercise Results</h1>
        </div>
    </header>

    <main class="exercise-res-container">
        <div class="exercise-res-row">
            <div class="res-list-container overflow-auto"">
            <h3 class="equipment-title">Bands:</h3>
                <ul>
                    <% results.forEach(result=> { %>
                        <% if(result.equipment==='assisted' || result.equipment==='band' ||
                            result.equipment==='resistance band' ) { %>
                            <% let name=result.name %>
                                <% name.replace('', '%20' ) %>
                                    <li>
                                        <a href="/exercises/<%= name %> ">
                                            <%= result.name %>
                                        </a>
                                    </li>
                            <% } %>
                    <% }) %>
                </ul>
            </div>
            <div class="res-list-container overflow-auto"">
            <h3 class="equipment-title">Barbells/Trap Bar:</h3>
                <ul>
                    <% results.forEach(result=> { %>
                        <% if(result.equipment==='barbell' || result.equipment==='ez barbell' ||
                            result.equipment==='olympic barbell' || result.equipment==='trap bar' ) { %>
                            <% let name=result.name %>
                                <% name.replace('', '%20' ) %>
                                    <li>
                                        <a href="/exercises/<%= name %> ">
                                            <%= result.name %>
                                        </a>
                                    </li>
                            <% } %>
                        <% }) %>
                </ul>
            </div>
            <div class="res-list-container overflow-auto"">
            <h3 class="equipment-title">Bodyweight:</h3>
                <ul>
                    <% results.forEach(result=> { %>
                        <% if(result.equipment==='body weight' ) { %>
                            <% let name=result.name %>
                                <% name.replace('', '%20' ) %>
                                    <li>
                                        <a href="/exercises/<%= name %> ">
                                            <%= result.name %>
                                        </a>
                                    </li>
                            <% } %>
                        <% }) %>
                </ul>
            </div>
        </div>

        <div class="exercise-res-row">
            <div class="res-list-container overflow-auto"">
            <h3 class="equipment-title">Cable:</h3>
                <ul>
                    <% results.forEach(result=> { %>
                        <% if(result.equipment==='cable' ) { %>
                            <% let name=result.name %>
                                <% name.replace('', '%20' ) %>
                                    <li>
                                        <a href="/exercises/<%= name %> ">
                                            <%= result.name %>
                                        </a>
                                    </li>
                            <% } %>
                        <% }) %>
                </ul>
            </div>
            <div class="res-list-container overflow-auto"">
            <h3 class="equipment-title">Dumbbell/Plates:</h3>
                <ul>
                    <% results.forEach(result=> { %>
                        <% if(result.equipment==='dumbbell' || result.equipment==='weighted' ) { %>
                            <% let name=result.name %>
                                <% name.replace('', '%20' ) %>
                                    <li>
                                        <a href="/exercises/<%= name %> ">
                                            <%= result.name %>
                                        </a>
                                    </li>
                            <% } %>
                        <% }) %>
                </ul>
            </div>
            <div class="res-list-container overflow-auto"">
            <h3 class="equipment-title">Kettlebell:</h3>
                <ul>
                    <% results.forEach(result=> { %>
                        <% if(result.equipment==='kettlebell' ) { %>
                            <% let name=result.name %>
                                <% name.replace('', '%20' ) %>
                                    <li>
                                        <a href="/exercises/<%= name %> ">
                                            <%= result.name %>
                                        </a>
                                    </li>
                            <% } %>
                        <% }) %>
                </ul>
            </div>
        </div>

        <div class="exercise-res-row">
            <div class="res-list-container overflow-auto"">
            <h3 class="equipment-title">Medicine/Bosu/Stability Ball:</h3>
                <ul>
                    <% results.forEach(result=> { %>
                        <% if(result.equipment==='bosu ball' || result.equipment==='stability ball' ||
                            result.equipment==='medicine ball' ) { %>
                            <% let name=result.name %>
                                <% name.replace('', '%20' ) %>
                                    <li>
                                        <a href="/exercises/<%= name %> ">
                                            <%= result.name %>
                                        </a>
                                    </li>
                                    <% } %>
                                        <% }) %>
                </ul>
            </div>
            <div class="res-list-container overflow-auto"">
            <h3 class="equipment-title">Smith Machine:</h3>
                <ul>
                    <% results.forEach(result=> { %>
                        <% if(result.equipment==='smith machine' ) { %>
                            <% let name=result.name %>
                                <% name.replace('', '%20' ) %>
                                    <li>
                                        <a href="/exercises/<%= name %> ">
                                            <%= result.name %>
                                        </a>
                                    </li>
                                    <% } %>
                                        <% }) %>
                </ul>
            </div>
            <div class="res-list-container overflow-auto"">
            <h3 class="equipment-title">Other:</h3>
                <ul>
                    <% results.forEach(result=> { %>
                        <% if(result.equipment==='elliptical machine' || result.equipment==='hammer' ||
                            result.equipment==='leverage machine' || result.equipment==='roller' ||
