                            result.equipment==='tire' || result.equipment==='upper body ergometer' ||
                            result.equipment==='wheel roller' ) { %>
                            <% let name=result.name %>
                                <% name.replace('', '%20' ) %>
                                    <li>
                                        <a href="/exercises/<%= name %> ">
                                            <%= result.name %>
                                        </a>
                                    </li>
                                    <% } %>
                                        <% }) %>
                </ul>
            </div>
        </div>
    </main>
    <div id="addedSpace"></div>

